<resources>

    <style name="line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">#e8e8e8</item>
    </style>

    <style name="radio_btn_tag">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@drawable/datecate_text_color</item>
        <item name="android:background">@drawable/datecate_tag_bg</item>
    </style>

    <style name="btn">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="btn.tip">
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:layout_marginRight">30dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/btn_corner</item>
    </style>

    <style name="btn.supervised_weight">
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/custom_btn_selector</item>
    </style>

    <style name="btn.yellow_weight">
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/btn_yellow_corner</item>
    </style>

    <style name="btn.black_weight">
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/btn_black_corner</item>
    </style>

    <style name="btn.supervised">
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_width">216dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/btn_corner</item>
    </style>

    <style name="text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="text.15_000000">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="text.14_5C5C5D">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/color_5C5C5D</item>
    </style>

    <style name="text.20_000000">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="text.15_979EAB">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#979EAB</item>
    </style>

    <style name="text.12_475162">
        <item name="android:textColor">#475162</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="text.11_555555">
        <item name="android:textColor">#555555</item>
        <item name="android:textSize">11sp</item>
    </style>

    <style name="text.16_232A39">
        <item name="android:textColor">#232A39</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="text.24_060720">
        <item name="android:textColor">#060720</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="text.24_2BA5FF">
        <item name="android:textColor">@color/color_2BA5FF</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="text.24_BDBDBD">
        <item name="android:textColor">@color/color_BDBDBD</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="text.12_060720">
        <item name="android:textColor">#060720</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="text.12_FEAA18">
        <item name="android:textColor">#FEAA18</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="text.patrol_control">
        <item name="android:layout_width">70dp</item>
        <item name="android:textColor">@color/color_232A39</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
        <item name="android:drawablePadding">2dp</item>
    </style>

    <style name="text.supervised_time">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#7D8594</item>
    </style>

    <style name="text.supervised_content">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#475162</item>
    </style>

    <style name="input_edit_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">66dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">@drawable/bg_border_stroke_8</item>
        <item name="android:gravity">top</item>
        <item name="android:textColor">@color/color_475162</item>
        <item name="android:textColorHint">@color/color_999999</item>
        <item name="android:textSize">12sp</item>
        <item name="android:hint">请输入描述</item>
        <item name="android:padding">8dp</item>
    </style>
</resources>
